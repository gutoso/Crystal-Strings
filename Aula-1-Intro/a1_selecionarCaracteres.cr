text = "The black cat climbed the green three"

puts text[0] # Seleciona o primeiro caractere, pois o "0" representa a primeira posição

puts text[0, 4] # Seleciona os cinco primeiros caracteres

puts text[0..5] # Seleciona os caracteres que vão da primeira casa até a quinta casa, ou seja, os seis primeiros caracteres

puts text[0..-3] # Seleciona todos os caracteres, exceto os três que estão nas últimas três casas
